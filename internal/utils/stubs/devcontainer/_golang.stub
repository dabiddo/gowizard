{
    "name":"$DIR",
    "build": {
        "dockerfile": "Dockerfile"
    },
    "remoteUser": "vscode",
    "customizations": {
        "vscode": {
            "extensions": [
                "golang.go",
                "ms-vscode.remote-containers",
                "3timeslazy.vscodium-devpodcontainers",
                "ms-azuretools.vscode-docker"
            ]
        }
    },
    "postCreateCommand": "go mod init $DIR"
}